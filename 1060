import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		
		Scanner sc = new Scanner (System.in);
		int i, c;
		float n;
		c = 0;
		
		for (i=0; i<6; i++)
		{
			n = sc.nextFloat();
			if (n>0)
			{
				c ++;
			}
		}
		System.out.println(c + " valores positivos");
	}
}
