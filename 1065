import java.text.DecimalFormat;
import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		
		Scanner sc = new Scanner (System.in);
		int i, n, par;
		par=0;
		for(i=0;i<5;i++)
		{
			n=sc.nextInt();
			if (n%2==0)
			{
				par++;
			}
			
		}
		System.out.println(par+" valores pares");
		sc.close();
	}
}
