import java.util.Scanner;
import java.text.DecimalFormat;

public class Main {

	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		int i, n, qdr;

		n = sc.nextInt();
		
		for (i=0; i<n; i++)
		{
			if (i>0 && i%2==0)
			{
				qdr = (int)Math.pow(i, 2);
				System.out.println(i + "^2"  + " = " + qdr);
			}
		}
		if (i%2==0)
		{
			qdr = (int)Math.pow(i, 2);
			System.out.println(i + "^2" + " = " + qdr);
		}
	}
}
