import java.util.Scanner;
import java.text.DecimalFormat;

public class Main {

	public static void main(String[] args) {
		
		DecimalFormat df = new DecimalFormat ("0.0");
		Scanner sc = new Scanner(System.in);
		int i, n;
		n = sc.nextInt();
		
		for (i=2; i<10000; i++)
		{
			
			if(i%n==2)
			{
				System.out.println(i);
			}
		}
	}
}
