import java.util.Scanner;
import java.text.DecimalFormat;

public class Main {

	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		int i, n, res;
		
		n = sc.nextInt();
		
		for (i=1; i<11; i++)
		{
			res = i*n;
			System.out.println(i + " x " + n + " = " + res);
		}
	}
}
