import java.util.Scanner;
import java.text.DecimalFormat;

public class Main {

	public static void main(String[] args) {
		
		int j=7;
		for(int i=1; i<11; i=i+2) {
		    System.out.println("I="+i+" J="+j);
		    j=j-1;
		    System.out.println("I="+i+" J="+j);
		    j=j-1;
		    System.out.println("I="+i+" J="+j);
		    j=j+4;
		}		
	}
}
