import java.util.Scanner;
import java.text.DecimalFormat;

public class Main {

	public static void main(String[] args) {
		
		DecimalFormat df = new DecimalFormat ("0.00");
		Scanner sc = new Scanner(System.in);
		int i, n, ln;
		ln=0;
		n=sc.nextInt();
		
		for(i=1; ln<n; i++)
		{
			if(i%4==0)
			{
				System.out.println("PUM");
				ln++;
			}
			else
			{
				System.out.print(i + " ");
			}
		}		
	}
}
