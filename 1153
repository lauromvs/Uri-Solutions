import java.util.Scanner;
import java.text.DecimalFormat;

public class Main {

	public static void main(String[] args) {
		
		DecimalFormat df = new DecimalFormat ("0.00");
		Scanner sc = new Scanner(System.in);
		int i, n, fat;
		n = sc.nextInt();
		fat = 1;
		
		for(i=2; i<=n; i++)
		{
			fat = fat*i;
		}
		System.out.println(fat);
	}
}
