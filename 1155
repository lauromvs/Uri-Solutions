import java.util.Scanner;
import java.text.DecimalFormat;

public class Main {

	public static void main(String[] args) {
		
		DecimalFormat df = new DecimalFormat ("0.00");
		Scanner sc = new Scanner(System.in);
		int i;
		double s;
		s=0;
		for(i=1; i<=100; i++)
		{
			s += 1.0/i;
		}
		System.out.println(df.format(s));
	}
}
