import java.util.Scanner;

public class Main {

	public static void main(String[] args) {

		Scanner sc = new Scanner(System.in);
		int i, n, hmax, hmin, h, hn;
		

		while(sc.hasNext())
		{
			n=sc.nextInt();
			hmin=sc.nextInt();
			hmax=sc.nextInt();
			hn=0;
			for(i=0;i<n;i++)
			{
				h=sc.nextInt();
				if (h<=hmax && h>=hmin)
				{
					hn++;
				}
			}
			System.out.println(hn);
		}
		sc.close();
	}
}
